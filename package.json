  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "deploy": "npx prisma deploy --env-file ./.env.local",
    "deploy:prod": "npx prisma deploy --env-file ./.env.production",
    "migrate:prod": "npx prisma deploy --force --env-file ./.env.production && npx prisma reset --force --env-file ./.env.production && yarn importer:prod && yarn dev",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "serve": "pm2-runtime start pm2.json",
    "importer:dev": "nodemon --exec yarn importer",
    "importer:prod": "PRISMA_ENDPOINT=http://10.0.11.126:4466/dev/one babel-node scripts/import.js",
    "importer": "PRISMA_ENDPOINT=http://localhost:4466/dev/one babel-node scripts/import.js",
    "migrate": "npx prisma deploy --env-file ./.env.local && npx prisma reset --force --env-file ./.env.local && yarn importer && yarn dev",
    "start-service-accounts": "ts-node-dev --no-notify --respawn --transpileOnly src/new/prisma.ts",
    "start-services": "concurrently \"npm:start-service-*\"",
    "predevelop": "yarn generate",
    "develop": "concurrently yarn:ds-*",
    "ds-build": "webpack --config webpack.development.js",
    "ds-serve": "sleep 10 && nodemon local/main.bundle.js",
    "generate": "graphql-codegen --config ./codegen.yml"
  },
  "devDependencies": {
